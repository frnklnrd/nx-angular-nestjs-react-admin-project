{
  "version": 2,
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/vercel",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/vercel/index.html",
      "methods": ["GET"]
    }
  ]
}
