{
  "version": 2,
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/(.*\\.css)",
      "dest": "/apps/api/src/assets/:1",
      "methods": ["GET"]
    },
    {
      "src": "/api/(.*\\.png)",
      "dest": "/apps/api/src/assets/:1",
      "methods": ["GET"]
    },
    {
      "src": "/api/swagger-ui-bundle\\.js",
      "dest": "/apps/api/src/assets/swagger-ui-bundle.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/swagger-ui-standalone-preset\\.js",
      "dest": "/apps/api/src/assets/swagger-ui-standalone-preset.js",
      "methods": ["GET"]
    }
  ]
}
