{
  "version": 2,
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)(?<!\\.css|\\.png|swagger-ui-bundle\\.js|swagger-ui-standalone-preset\\.js)$",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
